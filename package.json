{
  "name": "elaastic-client",
  "version": "0.0.1",
  "description": "Elaastic est un EIAH (Environnement Informatique pour l’Apprentissage Humain) dédiée à l’orchestration de séquences d’évaluations formatives pendant les cours en face à face ou à distance. Nos travaux de recherche portent sur la conception de systèmes technologiques permettant d’améliorer la mise en oeuvre de telles séquences pédagogiques dans le contexte d’un enseignement de masse.",
  "productName": "Elaastic",
  "author": "John Tranier <john.tranier@ticetime.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@lukemorales/query-key-factory": "^1.1.0",
    "@quasar/extras": "^1.0.0",
    "@tanstack/vue-query": "^4.24.6",
    "axios": "^0.21.1",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "semantic-ui-step": "^2.5.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.2.1",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitest/ui": "^0.29.1",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vitest": "^0.29.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
